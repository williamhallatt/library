{
  "name": "aws-sdk-utilities",
  "version": "1.0.0",
  "description": "Snippets and helper functions utilising the AWS JavaScript SDK",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "compile:lambda:prod": "webpack --config ./webpack.prod.js",
    "compile:lambda:dev": "webpack --config ./webpack.dev.js",
    "lint": "tslint --fix --project ./tsconfig.json",
